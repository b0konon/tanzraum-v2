{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Kurse"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "starttime": {
      "type": "string",
      "default": "08:00",
      "required": true,
      "regex": "^([01]\\d|2[0-3]):[0-5]\\d$"
    },
    "endtime": {
      "type": "string",
      "default": "09:00",
      "required": true,
      "regex": "^([01]\\d|2[0-3]):[0-5]\\d$"
    },
    "trainer": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "level": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    }
  }
}
